FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 223;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 223;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 223;
        value           uniform 223;
    }
    bottom
    {
        type            symmetry;
    }
    top
    {
        type            symmetry;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


