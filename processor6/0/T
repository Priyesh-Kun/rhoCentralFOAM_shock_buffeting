FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 223;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 223;
    }
    obstacle
    {
        type            zeroGradient;
    }
    bottom
    {
        type            symmetry;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    top
    {
        type            symmetry;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 223;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 223;
    }
}


